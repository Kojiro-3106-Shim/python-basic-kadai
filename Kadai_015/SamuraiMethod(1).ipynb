        "class Human:\n",
        "# クラス定義\n",
        "    def __init__(self,name,age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "#出力メソッド定義\n",
        "    def printinto(self):\n",
        "        print(self.name)\n",
        "        print(self.age)\n",
        "\n",
        "# インスタンス化\n",
        "human = Human(\"侍太郎\",36)\n",
        "\n",
        "#メソッド呼び出し\n",
        "human.printinto()\n"
      ]
