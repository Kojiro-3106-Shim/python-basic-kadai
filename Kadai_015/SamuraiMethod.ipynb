        "class Product:\n",
        "# クラス定義\n",
        "    def __init__(self):\n",
        "        self.name = \"\"\n",
        "        self.age = 0\n",
        "\n",
        "#出力メソッド定義\n",
        "    def printinto(self):\n",
        "        print(self.name)\n",
        "        print(self.age)\n",
        "\n",
        "# インスタンス化\n",
        "human = Product()\n",
        "\n",
        "# 出力メソッド用\n",
        "human.name = \"侍太郎\"\n",
        "human.age = 36\n",
        "human.printinto()\n"
      ]
    }
